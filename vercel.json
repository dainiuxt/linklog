{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {
        "serverFiles": [
          "server-middleware/**"
        ]
      }

    }
  ],
  "routes": [
    {
      "src": "https://wdf.driggl.now.sh/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://webdevflow.com"
      }
    }
  ],
  "env": {
    "API_URL": "@wdf-api-url"
  },
  "build": {
    "env": {
      "API_URL": "@wdf-api-url",
      "GITHUB_ID": "@wdf-github-id",
      "GITHUB_SECRET": "@wdf-github-secret",
      "GITHUB_REDIRECT_URI": "@wdf-github-redirect-uri"
    }
  }
}
